    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/../src/$1"
    },
    "coveragePathIgnorePatterns": [
      ".dto.ts",
      ".event.ts",
      ".constants.ts",
      ".config.ts",
      ".entity.ts",
      ".module.ts",
      ".index.ts",
      ".seeder.ts",
      ".strategy.ts",
      "<rootDir>/main.ts",
      "<rootDir>/seeder.ts",
      "<rootDir>/types.d.ts"
    ]
  }
}
